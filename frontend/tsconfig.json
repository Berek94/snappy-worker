{
  "compilerOptions": {
    "module": "ESNext",
    "target": "ES6",
    "noImplicitAny": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "lib": ["ESNext", "DOM"],
    "noImplicitReturns": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "strict": true,
    "jsx": "react",
    "moduleResolution": "Node"
  }
}
